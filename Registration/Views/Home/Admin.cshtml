
@{
    ViewBag.Title = "Admin";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Admin Report</h1>

<table class="table table-striped table-bordered">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Address1</th>
        <th>Address2</th>
        <th>City</th>
        <th>State</th>
        <th>Zip</th>
        <th>Country</th>
        <th>Date</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>@item.Address1</td>
            <td>@item.Address2</td>
            <td>@item.City</td>
            <td>@item.State</td>
            <td>@item.Zipcode</td>
            <td>@item.Country</td>
            <td>@item.DateTime.ToString()</td>
        </tr>
    }
</table>
